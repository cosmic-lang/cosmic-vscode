{
  "name": "ghoose-vscode",
  "displayName": "Ghoose Programming Language",
  "description": "Ghoose Programming Language",
  "version": "0.0.1",
  "homepage": "https://www.ghoose-lang.org",
  "respository": {
     "type": "git",
