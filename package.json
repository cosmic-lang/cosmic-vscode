{
  "name": "conjure-vscode",
  "displayName": "Conjure Programming Language",
  "description": "Conjure Programming Language",
  "version": "0.0.1",
  "homepage": "https://www.conjure-lang.org",
  "respository": {
     "type": "git",
     "url": "https://github.com/conjure-lang/conjure-vscode"
  },
  "icon": "assets/branding/png/conjure-transparent-main-icon-2.png",
  "license": ["MIT", "APACHE"],
  "publisher": "conjure-lang",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "conjure",
      "aliases": ["Conjure", "conjure"],
      "extensions": [".conjure", ".cnj"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "conjure",
      "scopeName": "source.conjure",
      "path": "./syntaxes/conjure.tmLanguage.json"
    }]
  }
}
